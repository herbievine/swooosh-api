       "yup": "^0.29.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.7",
        "@types/express": "^4.17.8",
        "@types/helmet": "0.0.48",
        "@types/mongoose": "^5.7.36",
        "@types/morgan": "^1.9.1",
        "@types/yup": "^0.29.7",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.2"
